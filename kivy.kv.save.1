<SwitchScreen>:
	name: "switch"
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'background.png'
	on_leave: root.resetButtonStates()
	on_pre_enter:
		root.updateTitle()
		root.updateButtons()
	FloatLayout:
		Label:
			id: frqlabel
			text: "Freq:"
			pos_hint: {"center_x":(0.3/8) + .01, "center_y":.55}
		Label:
			id: dclabel
			text: "DCyc:"
			pos_hint: {"center_x":(0.3/8) + .01, "center_y":.45}
		Label:
			id: btn0info
			text: root.getbtnInfo(0)[0]
			pos_hint: {"center_x":(0.3/8)+.05, "center_y":.5}
		Label:
			id: btn1info
			text: root.getbtnInfo(0)[1]
			pos_hint: {"center_x":(0.3/8)+.05, "center_y":.4}
		Button:
			text: "Back"
			on_press:
			on_release: app.root.current = "chooseprog"
            color: 1,1,1,1
            font_size: 25
            size_hint: 0.15,0.15
            pos_hint: {"center_x":.1, "center_y":.1}
        ToggleButton:
			id: btn0
			on_press: root.btn0Out()
			text: root.getbtnTitle(0)
            color: 1,1,1,1
            font_size: 25
            size_hint: 0.13,0.4
            pos_hint: {"center_x":(0.3/8)+.05, "center_y":.78}
        ToggleButton:
