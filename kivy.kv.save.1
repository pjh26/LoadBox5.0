#: import NoTransition kivy.uix.screenmanager.NoTransition

ScreenManagement:
	transition: NoTransition()
	HomeScreen:
	ChooseProgScreen:
    SwitchScreen:
    NewProgScreen0:
	NewProgScreen1:
	EditProgScreen:
	DeleteProgScreen:


<HomeScreen>:
	name: "home"
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'background.png'
	FloatLayout:

		Button:
			text: "Select Program"
			on_press: root.setFunc(0)
			on_release: app.root.current = "chooseprog"
            color: 1,1,1,1
            font_size: 25
            size_hint: 0.5,0.2
            pos_hint: {"center_x":.5, "center_y":.82}

        Button:
			text: "Add New Program"
			on_release: app.root.current = "newprog0"
            color: 1,1,1,1
            font_size: 25
            size_hint: 0.5,0.2
            pos_hint: {"center_x":.5, "center_y":.61}

		Button:
			text: "Delete Existing Program"
			on_press: root.setFunc(2)
			on_release: app.root.current = "chooseprog"
            color: 1,1,1,1
            font_size: 25
            size_hint: 0.5,0.2
            pos_hint: {"center_x":.5, "center_y":.4}

		Button:
			text: "Exit"
			on_press: app.stop()
            color: 1,1,1,1
            font_size: 25
            size_hint: 0.1,0.1
            pos_hint: {"center_x":.1, "center_y":.9}

