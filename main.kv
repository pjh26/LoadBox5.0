#: import NoTransition kivy.uix.screenmanager.NoTransition
#: import Factory kivy.factory.Factory
ScreenManagement:
	transition: NoTransition()
	HomeScreen
	ChooseProgScreen
	SwitchScreen
	NewProgScreen
	EditProgScreen

<HomeScreen>:
	name: "home"
	canvas.before:
		Rectangle:
			pos: self.pos
			size: self.size
			source: 'background.png'
	FloatLayout:

		Button:
			text: "Select Program"
			on_press: root.setFunc(0)
			on_release: app.root.current = "chooseprog"
			color: 1,1,1,1
			font_size: 50
			size_hint: 0.5,0.2
			pos_hint: {"center_x":.5, "center_y":.82}

		Button:
			text: "Add New Program"
			on_release: app.root.current = "newprog"
			color: 1,1,1,1
			font_size: 50
			size_hint: 0.5,0.2
			pos_hint: {"center_x":.5, "center_y":.61}

		Button:
			text: "Edit Existing Program"
			on_press: root.setFunc(1)
			on_release: app.root.current = "chooseprog"
			color: 1,1,1,1
			font_size: 50
			size_hint: 0.5,0.2
			pos_hint: {"center_x":.5, "center_y":.4}
		Button:
			text: "Delete Existing Program"
			on_press: root.setFunc(2)
			on_release: app.root.current = "chooseprog"
			color: 1,1,1,1
			font_size: 50
			size_hint: 0.5,0.2
			pos_hint: {"center_x":.5, "center_y":.19}

		Button:
			text: "Exit"
			on_press: app.stop()
			color: 1,1,1,1
			font_size: 50
			size_hint: 0.1,0.1
			pos_hint: {"center_x":.1, "center_y":.9}

<ChooseProgScreen>:
	popup: popup.__self__
	name: "chooseprog"
	on_pre_enter: root.updateButtons()
	canvas.before:
		Rectangle:
			pos: self.pos
			size: self.size
			source: 'background.png'
	FloatLayout:
		id: bl

		Label:
			id: lblTitle
			text: root.getLblTxt()
			pos_hint: {"center_x":.5, "center_y":.1}
			font_size: 50
		Button:
			text: "Back"
			on_release: app.root.current = "home"
			color: 1,1,1,1
			font_size: 50
			size_hint: 0.15,0.15
			pos_hint: {"center_x":.1, "center_y":.1}

		ScrollView:
			size_hint: 0.95, 0.775
			pos_hint: {'center_x': .5, 'center_y': .5875}

			GridLayout:
				id: chooseProgGrid
				cols: 4
				padding: 10
				spacing: 10
				height: self.minimum_height
				size_hint: 1, None
				do_scroll_x: False

		Popup:
			id: popup
			size_hint: .5,.3
			title: "Are you sure you want to delete?"
			on_parent:
				if self.parent == bl: self.parent.remove_widget(self)
			FloatLayout:
				Button:
					text: 'Ok'
					pos_hint: {"center_x":.25,"center_y":.5}
					size_hint: .4,.6
					font_size: 50
					on_release:
						root.setFunc()
						root.updateButtons()
						popup.dismiss()
				Button:
					text: 'Cancel'
					font_size: 50
					pos_hint: {"center_x":.75,"center_y":.5}
					size_hint: .4,.6
					on_release:
						popup.dismiss()

<ProgButton>:
	on_release: app.root.current = root.getScrn()
	height: 200
	color: 1,1,1,1
	background_color: 1,1,1,1
	font_size: 50
	size_hint_y: None

<LoadingPopup@Popup>:
	title: ""
	id: loadingPopup
	size_hint: 0.6, 0.1
	auto_dismiss: False
	pos_hint: {"center_x":.5, "center_y":.5}
	ProgressBar:
		id: progBar
		size_hint: None, None
		max: 100

